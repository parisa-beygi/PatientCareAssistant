<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/TurnOffAlarmPage_imageView_wallpaper"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="32dp"
        android:adjustViewBounds="false"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/loginpagewallpaper" />


    <RelativeLayout
        android:id="@+id/TurnOffAlarmPage_relativeLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/TurnOffAlarmPage_imageView_wallpaper"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/TurnOffAlarmPage_textView_toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#d6bb9d" />

        <ImageView
            android:id="@+id/TurnOffAlarmPage_iamgeView_logo"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_centerInParent="false"
            android:layout_marginEnd="325dp"
            android:layout_marginRight="325dp"
            app:srcCompat="@drawable/logo" />

    </RelativeLayout>


    <TextView
        android:id="@+id/TurnOffAlarmPage_textView_scheduleText"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/TurnOffAlarmPage_imageView_wallpaper"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/TurnOffAlarmPage_imageView_wallpaper"
        app:layout_constraintRight_toRightOf="@+id/TurnOffAlarmPage_imageView_wallpaper"
        app:layout_constraintTop_toBottomOf="@+id/TurnOffAlarmPage_relativeLayout"
        app:layout_constraintVertical_bias="0.17000002" />

    <Button
        android:id="@+id/TurnOffAlarmPage_button_alarm_off"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="turn off alarm"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/TurnOffAlarmPage_imageView_wallpaper"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/TurnOffAlarmPage_imageView_wallpaper"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/TurnOffAlarmPage_imageView_wallpaper"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/TurnOffAlarmPage_relativeLayout"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintVertical_bias="0.57" />

    <TextView
        android:id="@+id/TurnOffAlarmPage_textView_updateText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Did you set the alarm?"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/TurnOffAlarmPage_relativeLayout"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/TurnOffAlarmPage_imageView_wallpaper"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.71000004"
        app:layout_constraintHorizontal_bias="0.504" />


</android.support.constraint.ConstraintLayout>